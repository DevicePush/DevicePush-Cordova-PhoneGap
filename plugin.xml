<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="com.devicepush.cordova-phonegap"
        version="0.3.0">

	<name>DevicePush</name>
	
	<engines>
		<engine name="cordova" version=">=3.0.0"/>
	</engines>
	
	<asset src="www/devicepush.min.js" target="js/devicepush.min.js"/>
	<js-module src="www/devicepush.min.js" name="devicePush">
		<clobbers target="devicePush" />
	</js-module>
  
    <dependency id="cordova-plugin-device" url="https://github.com/apache/cordova-plugin-device/" />
	<dependency id="phonegap-plugin-push" url="https://github.com/phonegap/phonegap-plugin-push.git" />
	<dependency id="cordova-plugin-geolocation" url="https://github.com/apache/cordova-plugin-geolocation/" />
  
</plugin>